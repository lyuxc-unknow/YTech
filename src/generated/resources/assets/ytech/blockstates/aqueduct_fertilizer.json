{
  "multipart": [
    {
      "apply": {
        "model": "ytech:block/aqueduct_fertilizer"
      },
      "when": {
        "waterlogged": "false"
      }
    },
    {
      "apply": {
        "model": "ytech:block/aqueduct_fertilizer_waterlogged"
      },
      "when": {
        "waterlogged": "true"
      }
    },
    {
      "apply": {
        "model": "ytech:block/aqueduct_fertilizer_overlay"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "ytech:block/aqueduct_fertilizer_overlay",
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "ytech:block/aqueduct_fertilizer_overlay",
        "y": 270
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "ytech:block/aqueduct_fertilizer_overlay",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    }
  ]
}